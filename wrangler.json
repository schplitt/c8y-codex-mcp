{
  "compatibility_date": "2026-02-17",
  "name": "c8y-codex-mcp",
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": [
        "CodexMcpAgent"
      ]
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "MCP_OBJECT",
        "class_name": "CodexMcpAgent"
      }
    ]
  },
  "kv_namespaces": [
    {
      "binding": "CACHE"
    }
  ],
  "browser": {
    "binding": "BROWSER"
  },
  "workers_dev": true,
  "main": "./server/index.ts",
  "upload_source_maps": true,
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "observability": {
    "logs": {
      "enabled": false,
      "invocation_logs": true
    }
  }
}